{
  "name": "vite-project",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite --mode dev --port 1234 --host 0.0.0.0",
    "prod": "vite --mode prod --port 1234 --host 0.0.0.0",
    "build": "./build.sh",
    "publish": "./publish.sh",
    "publish-dev": "VITE_ENV=dev npm run publish",
    "publish-prod": "VITE_ENV=prod npm run publish",
    "build-dev": "VITE_ENV=dev npm run build",
    "build-prod": "VITE_ENV=prod npm run build",
    "preview": "vite preview",
    "boilerplate": "node boilerplate.cjs",
    "format": "prettier --write \"src/**/*.vue\"",
    "eslint": "eslint \"src/**/*.{ts,vue}\""
  },
  "dependencies": {
    "@headlessui/vue": "^1.7.14",
    "@heroicons/vue": "^2.0.18",
    "date-fns": "^2.30.0",
    "vue": "^3.3.4",
    "vue-router": "^4.2.4"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^6.2.1",
    "@typescript-eslint/parser": "^6.2.1",
    "@vitejs/plugin-vue": "^4.2.3",
    "@vitejs/plugin-vue-jsx": "^3.0.1",
    "autoprefixer": "^10.4.14",
    "eslint": "^8.46.0",
    "eslint-plugin-vue": "^9.16.1",
    "postcss": "^8.4.27",
    "prettier": "^3.0.1",
    "tailwind-scrollbar": "^3.0.5",
    "tailwindcss": "^3.3.3",
    "typescript": "^5.0.2",
    "vite": "^4.4.5",
    "vue-tsc": "^1.8.5"
  }
}
